    <script type="text/javascript">
    	$(function(){
    		var FWLXID="${pd.FWLXID}";
    		var stageIndex=0;
    		var html="";
    		if(FWLXID=="选址、条件"){
    			html+='<option selected="selected" value="选址、条件">选址、条件</option>';
    			stageIndex=0;
    		}
    		if(FWLXID=="用地"){
    			html+='<option selected="selected" value="用地">用地</option>';
    			stageIndex=1;
    		}
    		if(FWLXID=="建设许可（建筑工程）"){
    			html+='<option selected="selected" value="建设许可（建筑工程）">建设许可（建筑工程）</option>';
    			stageIndex=2;
    		}
    		if(FWLXID=="建设许可（市政道桥）"){
    			html+='<option selected="selected" value="建设许可（市政道桥）">建设许可（市政道桥）</option>';
    			stageIndex=3;
    		}
    		if(FWLXID=="建设许可（市政管线）"){
    			html+='<option selected="selected" value="建设许可（市政管线）">建设许可（市政管线）</option>';
    			stageIndex=4;
    		}
    		if(FWLXID=="竣工（建筑工程）"){
    			html+='<option selected="selected" value="竣工（建筑工程）">竣工（建筑工程）</option>';
    			stageIndex=5;
    		}
    		if(FWLXID=="竣工（市政道桥）"){
    			html+='<option selected="selected" value="竣工（市政道桥）">竣工（市政道桥）</option>';
    			stageIndex=6;
    		}
    		if(FWLXID=="竣工（市政管线）"){
    			html+='<option selected="selected" value="竣工（市政管线）">竣工（市政管线）</option>';
    			stageIndex=7;
    		}
    		$("#FWLXID").html(html);
    		var infoDivs=$(".infoDiv");
   			for(var i=0;i<infoDivs.length;i++){
   				infoDivs[i].style.display="none";
   				$(".infoDiv:eq("+i+")").addClass("unDisDiv");
   			}
   			infoDivs[stageIndex].style.display="block";
   			$(".infoDiv:eq("+(stageIndex)+")").removeClass("unDisDiv");
   			//建筑物使用性质回显值
   			var JZWSYXZ="${pd.JZWSYXZ}";
   			if(null!=JZWSYXZ&&JZWSYXZ!=""&&typeof(JZWSYXZ)!="undefiend"){
   				$("#JZWSYXZ").val(JZWSYXZ.split(',')).trigger("change");
   			}
   			$("form input").prop("readonly",true);
   			$("form textarea").prop("readonly",true);
   			$("form select").prop("disabled","disableds");
    	});
   		//保存按钮点击事件
   		$("#save").on("click",function(){
   			
   			if($("#XMWYM").val().trim()==""){
   				layer.tips("请填写项目唯一码","#XMWYM",{
   					tips:[1,'#3595CC'],
   					time:3000
   				});
   				return false;
			}
			if($("#XMMC").val().trim()==""){
   				layer.tips("请填写项目名称","#XMMC",{
   					tips:[1,'#3595CC'],
   					time:3000
   				});
   				return false;
			}
			if($("#DAHH").val().trim()==""){
   				layer.tips("请填写档案盒号","#DAHH",{
   					tips:[1,'#3595CC'],
   					time:3000
   				});
   				return false;
			}
			if($("#FWWH").val().trim()==""){
   				layer.tips("请填写发文文号","#FWWH",{
   					tips:[1,'#3595CC'],
   					time:3000
   				});
   				return false;
			}
			if($("#FWRQ").val().trim()==""){
   				layer.tips("请填写发文日期","#FWRQ",{
   					tips:[1,'#3595CC'],
   					time:3000
   				});
   				return false;
			}
   			var newUrl='sphz/fawen/edit.do?FWLXID='+$("#FWLXID").val();
   			//alert($("#JZWSYXZ").val());
   			if($("#FWLXID").val()=="建设许可（建筑工程）"){
   				newUrl='sphz/fawen/edit.do?FWLXID='+$("#FWLXID").val()+'&JZWSYXZ='+$("#JZWSYXZ").val();
   			}
   			$("#addForm").attr('action',newUrl);
			//$(".unDisDiv").remove();//删除display=none的发文信息Div
			$(".unDisDiv").remove();//删除display=none的发文信息Div
			var index = layer.load(0, {shade: false}); //0代表加载的风格，支持0-2
			$("#addForm").ajaxSubmit(function(data){
    			//console.log(data);
    			if(data.result=="success"){
    				layer.msg('保存成功', {icon: 1});
    				var index = parent.layer.getFrameIndex(window.name);
    				parent.layer.close(index); 
    				window.parent.location.reload();//刷新父页面
    			}else{
    				layer.msg('保存失败,'+data.result, {icon: 1});
    			}
    		});
   		})
   		
    	//报建类型切换更换发证信息内容
    	function changeStage(){
   			var stageIndex= $("#FWLXID").get(0).selectedIndex;//获取下拉框选中项的索引
   			var infoDivs=$(".infoDiv");
   			for(var i=0;i<infoDivs.length;i++){
   				infoDivs[i].style.display="none";
   				$(".infoDiv:eq("+i+")").addClass("unDisDiv");
   			}
   			infoDivs[stageIndex].style.display="block";
   			$(".infoDiv:eq("+(stageIndex)+")").removeClass("unDisDiv");
   		}
   		
   		function check(value){
			$.ajax({
				type: "POST",
				url: 'sphz/project/getProjectByXmwym',
		    	data: {XMWYM:value},
				dataType:'json',
				cache: false,
				success: function(data){
					if(data.result=="success"){
						var	proj= data.proj;
						$("#SSXMWYM").val(proj.SSXMWYM);
						$("#XMMC").val(proj.XMMC);
					}
				}
	 	 	 });
   		}

    </script>

  
