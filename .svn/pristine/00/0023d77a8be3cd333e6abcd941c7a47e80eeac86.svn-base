.mapnavibox-table .flat+span.switch {
    border: 1px solid #dfdfdf;
    transition: border 0.4s, box-shadow 0.4s;
    background-color: rgb(255, 255, 255);
    box-shadow: rgb(223, 223, 223) 0px 0px 0px 0px inset;
    background-color: rgb(255, 255, 255);
    cursor: pointer;
}

.mapnavibox-table .flat+span.switch span.slider {
    left: 0;
    transition: background-color 0.4s, left 0.2s;
}

.mapnavibox-table .flat:checked+span.switch {
    border: 1px solid #0073b7;
    box-shadow: #0073b7 0px 0px 0px 16px inset;
    transition: border 0.4s, box-shadow 0.2s, background-color 1.2s;
    background-color: #0073b7;
    cursor: pointer;
}

.mapnavibox-table .flat:checked+span.switch span.slider {
    left: 16px;
    transition: background-color 0.4s, left 0.2s;
}


