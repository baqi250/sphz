/* ----- 搜索功能 ----------------------------------------------------------------*/

#whmaptoolbar-searchbox {
    height: calc(100% - 50px);
    width: calc(100% + 50px - (100% - 100px) / 4);
    height: -webkit-calc(100% - 50px);
    width: -webkit-calc(100% + 50px - (100% - 100px) / 4);
    height: -moz-calc(100% - 50px);
    width: -moz-calc(100% + 50px - (100% - 100px) / 4);
    margin-top: 10px;
    pointer-events: all;
}

#mapsearchbox-inputbox {
    width: 100%;
    height: 34px;
    position: relative;
}


/* 搜索功能 输入框 */

#mapsearchbox-input {
    border-radius: 0;
    box-shadow: none;
    border: 1px solid #d2d6de;
    height: 100%;
    width: 100%;
    padding: 0 90px 0 30px;
    font-size: 13px;
}

#mapsearchbox-input:focus {
    border-color: #0073b7;
    box-shadow: 0;
    outline: none !important;
}

#mapsearchbox-i {
    display: inline-block;
    position: absolute;
    top: 50%;
    bottom: auto;
    left: 10px;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    overflow: hidden;
    color: #d2d6de;
}

#mapsearchbox-input:focus~#mapsearchbox-i {
    color: #0073b7;
}


/* 搜索功能 选择框 */

#mapsearchbox-select {
    display: inline-block;
    position: absolute;
    width: 70px;
    height: 30px;
    right: 10px;
    top: 50%;
    bottom: auto;
    font-size: 13px;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    overflow: hidden;
    border: none;
    outline: none;
    background-color: #fff;
    /* 隐藏select的下拉图标 */
    appearance: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    -o-appearance: none;
    -ms-appearance: none;
}

.cd-select:after {
    content: "";
    position: absolute;
    right: 20px;
    top: 45%;
    pointer-events: none;
    border: solid transparent;
    height: 0;
    width: 0;
    border-width: 4px;
    border-top-color: #333333;
}


/* 搜索功能 结果框 */

#mapsearchbox-result {
    width: 100%;
    height: calc(100% - 44px);
    height: -webkit-calc(100% - 44px);
    height: -moz-calc(100% - 44px);
    margin-top: 10px;
    padding: 10px;
    overflow-y: auto;
    background-color: #fff;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    border-radius: 2px;
    color: #333;
    line-height: 1.5;
}

#mapsearchbox-result .result-item {
    display: inline-block;
    width: 100%;
}

#mapsearchbox-result .no-result {
    text-align: center;
    font-style: italic;
    font-size: 13px;
}

#mapsearchbox-result .result-item ul {
    list-style: none;
    padding-left: 0;
}

#mapsearchbox-result .result-item a {
    display: inline-block;
    color: #333545;
}

#mapsearchbox-result .result-item a:hover {
    opacity: .6;
}

#mapsearchbox-result .result-item h4 {
    font-weight: bold;
    font-size: 14px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    max-width: 100%;
    padding: 5px 0 10px;
    margin: 0;
}

#mapsearchbox-result .result-item h4 b {
    color: #0073b7;
}

#mapsearchbox-result .result-item p {
    font-size: 12px;
    color: #70727d;
    margin: 0;
    max-width: 100%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

#mapsearchbox-result .result-item p b {
    color: #0073b7;
    opacity: .8;
}

#mapsearchbox-result .has-result li {
    position: relative;
    padding: 10px 10px 10px 60px;
    margin-bottom: 10px;
}

#mapsearchbox-result .has-result #result-item-ul {
    margin: 0 !important;
}

#mapsearchbox-result .has-result li:hover {
    background-color: rgba(0, 192, 239, 0.2);
}

#mapsearchbox-result .has-result li:last-of-type {
    margin-bottom: 0;
}

#mapsearchbox-result .has-result .image-wrapper {
    display: inline-block;
    position: absolute;
    left: 10px;
    top: 50%;
    bottom: auto;
    -webkit-transform: translateY(-50%);
    -moz-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    transform: translateY(-50%);
    width: 40px;
    height: 40px;
    background-color: #c0c0c0;
    background-position: center center;
    background-repeat: no-repeat;
    background-size: cover;
}


/* ----- identify功能 ----------------------------------------------------------------*/

#whmaptoolbar-idenbox {
