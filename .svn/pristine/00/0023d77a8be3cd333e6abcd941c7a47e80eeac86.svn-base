/* ----- 按钮组 ----------------------------------------------------------------*/

#whmaptoolbar-itemgroup {
    height: 50px;
    width: 100%;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item {
    pointer-events: all;
    text-align: center;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn {
    position: relative;
    width: 50px;
    height: 50px;
    background-color: #fff;
    border-radius: 2px;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.3);
    -moz-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.3);
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):hover {
    background-color: #f6f6f6;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):hover span i {
    color: #0073b7;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn.active {
    border: 2px solid #0073b7;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn span i {
    display: inline-block;
    position: absolute;
    top: 50%;
    bottom: auto;
    left: 15px;
    transform: translateY(-50%);
    -webkit-transform: translateY(-50%);
    -o-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    overflow: hidden;
    color: #666666;
    font-size: 20px;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn.active span i {
    color: #0073b7;
    left: 13px;
}


/* 按钮组 按钮动画效果 */

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):before,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):after,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active)>span:before,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active)>span:after {
    content: '';
    position: absolute;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active) span:before {
    border-left: 2px solid #0073b7;
    top: 0;
    bottom: 100%;
    left: 0;
    -webkit-transition-property: all;
    -moz-transition-property: all;
    -o-transition-property: all;
    transition-property: all;
    -webkit-transition-timing-function: cubic-bezier(0, 0.98, 0.51, 0.93);
    -moz-transition-timing-function: cubic-bezier(0, 0.98, 0.51, 0.93);
    -o-transition-timing-function: cubic-bezier(0, 0.98, 0.51, 0.93);
    transition-timing-function: cubic-bezier(0, 0.98, 0.51, 0.93);
    -webkit-transition-duration: 200ms;
    -moz-transition-duration: 200ms;
    -o-transition-duration: 200ms;
    transition-duration: 200ms;
    -webkit-transition-delay: 0ms;
    -moz-transition-delay: 0ms;
    -o-transition-delay: 0ms;
    transition-delay: 0ms;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):hover span:before {
    bottom: 0%;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):not(:hover) span:before {
    -webkit-transition-delay: 600ms;
    -moz-transition-delay: 600ms;
    -o-transition-delay: 600ms;
    transition-delay: 600ms;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):before,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):after,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active)>span:before,
#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active)>span:after {
    content: '';
    position: absolute;
}

#whmaptoolbar-itemgroup .whmaptoolbar-item .whmaptoobar-btn:not(.active):after {
    border-bottom: 2px solid #0073b7;
    left: 0;
