.panel {
    margin-bottom: 5px;
    background-color: #fff;
    border: 1px solid transparent;
    border-radius: 0px;
    -webkit-box-shadow: 0 1px 1px rgba(0,0,0,.05);
    box-shadow: 0 1px 1px rgba(0,0,0,.05);
}
.panel-heading{
	padding:7px 5px 5px 16px;
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
    vertical-align: middle;
}
.panel-primary {
    border-color: #CECDCD;
}
.panel-primary>.panel-heading {
    background-color: #DCF4D4;
    border-color: #9CC5B8;
    border-width: 1px;
    height: 34px;
}
.panel-title {
    font-weight: bold;
    color: #00563C;
}
.panel-footer{
	padding:5px;
}
.button-group{
	padding:0px 5px 5px 5px;
	/*background-color:#E5E8FB;
	border:solid 1px #BBD6F9;*/
}
