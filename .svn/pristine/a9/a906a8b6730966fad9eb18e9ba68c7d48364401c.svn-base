}
/*添加取消按钮样式2015-12-30*/
.btn-default, .btn-primary, .btn-primary:focus {
    color:#FFFFFF;
    background-color:#3D4A53;
    border-color:#3D4A53;
    padding:3px 15px;
    border-radius:0px;
    min-width: 70px;
}
.btn-primary:focus {
    outline: none;
}
/*设置按钮效果;添加取消按钮效果2015-12-30*/
.btn-default.active,.btn-default:hover,.btn-primary.active, .btn-primary:hover{
    color: #fff;
    background-color: #00aa76;
    border-color: #00aa76;
}
/*调整按钮样式2016-1-8*/
.btn-success {
    min-width: 70px;
    padding: 3px 15px;

}
.btn-group-justified {
    border-collapse: collapse;
    margin-top:5px;
}
.label-control{
    font-size:14px;
    font-weight:normal;
}
.form-group{
    margin-top:5px;    /*添加上边距2015-12-31*/
